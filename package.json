{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf dist && babel ./ -d dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "dev": "nodemon --exec babel-node src/index.js",
    "serve": "node dist/src/index.js",
    "start": "npm run build && npm run serve",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "axios": "^0.20.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "^1.8.0",
    "http-status": "^1.4.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.5.2",
    "nodemon": "^2.0.4"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".babelrc",
      ".eslintrc.json"
    ]
  }
}
